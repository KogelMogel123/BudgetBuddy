{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "AppSettings": {
    "MakeConfiguration": {
      "Url": "<MAKE_COM_ENDPOINT_URL>"
    },
    "ApiUsageSettings": {
      "ApiKeyHeaderName": "ApiKey",
      "UserHeaderName": "User",
      "RequestLimit": 5,
      "BlockDurationMinutes": 60,
      "ValidApiKey": "<YOUR_API_KEY>"
    }
  }
}